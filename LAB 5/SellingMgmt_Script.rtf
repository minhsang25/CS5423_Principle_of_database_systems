\pard\nowidctlpar\cf1\f0\fs24 create database SellingManagement\par
GO\par
use SellingManagement\par
GO\par
create table CUSTOMERS\par
(\par
CUST_ID CHAR(4) PRIMARY KEY,\par
NAME VARCHAR(40),\par
ADDRESS VARCHAR(50),\par
PHONE VARCHAR(20),\par
BIRTH_DATE SMALLDATETIME,\par
REG_DATE SMALLDATETIME,\par
\f1 TOTAL_ORDER \f0 MONEY\par
)\par
CREATE TABLE EMPLOYEES \par
(\par
EMP_ID CHAR(4) PRIMARY KEY,\par
EMP_NAME VARCHAR(40),\par
PHONE VARCHAR(20),\par
HIRE_DATE SMALLDATETIME\par
)\par
CREATE TABLE PRODUCTS\par
(\par
PRO_ID CHAR(4) PRIMARY KEY,\par
PRO_NAME VARCHAR(40),\par
UNIT VARCHAR(20),\par
COUNTRY VARCHAR(40),\par
PRICE MONEY\par
)\par
CREATE TABLE ORDERS\par
(\par
ORDER_ID INT PRIMARY KEY,\par
ORDER_DATE SMALLDATETIME,\par
CUST_ID CHAR(4),\par
EMP_ID CHAR(4),\par
TOTAL MONEY\par
)\par
Create table ORDER_ITEMS \par
(\par
\tab ORDER_ID\tab int foreign key\tab references ORDERS(ORDER_ID),\par
\tab PRO_ID\tab char(4) foreign key references PRODUCTS(PRO_ID),\par
\tab QUANTITY\tab\tab int,\par
\tab constraint PK_ORDER_ITEMS   primary key (ORDER_ID,PRO_ID)\par
)\par
ALTER TABLE ORDERS ADD CONSTRAINT FK_CUST_ID FOREIGN KEY(CUST_ID)\par
REFERENCES CUSTOMERS(CUST_ID)\par
ALTER TABLE ORDERS ADD CONSTRAINT FK_EMP_ID FOREIGN KEY(EMP_ID)\par
REFERENCES EMPLOYEES (EMP_ID)\par
